<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--Порт, который будет использоватся для пересылки сообщений-->
    <add key="UdpPort" value="1234"/>
    <!--Период подачи сигнала пристуствия в чате-->
    <add key="OnlineMarkerPeriod" value="300"/>
    <add key="AppMode" value="network"/>
  </appSettings>
</configuration>