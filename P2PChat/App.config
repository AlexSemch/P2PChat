<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--Порт, который будет использоватся для отправки сообщений-->
    <add key="LocalPort" value="1234"/>
    <!--Порт, который будет использоватся для приема сообщений-->
    <add key="RemotePort" value="1234"/>
    <!--Адрес для груповой рассылки про присутствия в чате-->
    <add key="GroupAddress" value="224.0.0.1"/>
    <!--Период подачи сигнала пристуствия в чате-->
    <add key="OnlineMarkerPeriod" value="3000"/>
    <!--Число маршрутизаторов, которые должны пересылать сообщение с докладом-->
    <add key="TTL" value="30"/>
  </appSettings>
  
</configuration>